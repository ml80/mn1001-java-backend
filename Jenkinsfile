pipeline {
  agent any
  stage('Build')
  {
    steps
    {
      {
        sh '''#!/bin/bash
        echo Build stage
        '''
      }
    }
    stage('Execute')
  {
    steps
    {
      {
        sh '''#!/bin/bash
        echo Execute stage
        '''
      }
    }
    stage('Test')
  {
    steps
    {
      {
        sh '''#!/bin/bash
        echo Test stage
        '''
      }
    }
    
   }
}
     
        
        
